{
  "name": "puppet-augeas",
  "version": "1.9.0",
  "author": "Vox Pupuli",
  "summary": "Puppet Augeas module",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-augeas",
  "project_page": "https://github.com/voxpupuli/puppet-augeas",
  "issues_url": "https://github.com/voxpupuli/puppet-augeas/issues",
  "dependencies": [
    {
      "name": "puppetlabs/augeas_core",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "9"
      ]
    },
    {
      "operatingsystem": "Archlinux"
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "13",
        "14"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 7.0.0 < 9.0.0"
    }
  ]
}
